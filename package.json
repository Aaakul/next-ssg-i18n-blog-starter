{
  "name": "next-ssg-i18n-blog-starter",
  "version": "0.2.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:webpack": "cross-env INIT_CWD=$PWD next dev --webpack",
    "dev": "npm run contentlayer && cross-env INIT_CWD=$PWD next dev",
    "export": "npm run lint && cross-env INIT_CWD=$PWD next build --webpack && npm run postbuild",
    "export:turbo": "npm run lint && npm run contentlayer && cross-env INIT_CWD=$PWD next build && npm run postbuild",
    "contentlayer": "contentlayer2 build",
    "postbuild": "next-sitemap && node ./scripts/genFeed.mjs",
    "analyze": "cross-env INIT_CWD=$PWD ANALYZE=true next build --webpack",
    "lint": "eslint --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@next/bundle-analyzer": "^16.0.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.15",
    "@types/probe-image-size": "^7.2.5",
    "@types/react-dom": "19.2.2",
    "autoprefixer": "^10.4.22",
    "body-scroll-lock": "^4.0.0-beta.0",
    "clsx": "^2.1.1",
    "contentlayer2": "^0.5.8",
    "feed": "^5.1.0",
    "github-slugger": "^2.0.0",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "js-cookie": "^3.0.5",
    "kbar": "^0.1.0-beta.48",
    "next": "^16.0.2",
    "next-contentlayer2": "^0.5.8",
    "next-intl": "^4.5.2",
    "next-themes": "^0.4.4",
    "postcss": "^8.5.6",
    "probe-image-size": "^7.2.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-wrap-balancer": "^1.1.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-katex-notranslate": "^1.1.4",
    "rehype-preset-minify": "7.0.1",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^2.0.0",
    "remark-math": "^6.0.0",
    "shiki": "^3.15.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.39.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/js-cookie": "^3.0.6",
    "@types/mdx": "^2.0.13",
    "@types/react": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx|mjs|cjs)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|mjs|cjs|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
